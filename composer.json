{
    "name": "phpugl/twitter-bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Twitter Bootstrap bundle for symfony",
    "keywords": ["bootstrap", "css", "js"],
    "authors": [
        {
            "name": "Julien 'ruian' Galenski"
        },
        {
          "name": "PHPUGL",
          "email": "info@phpugl.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
              "name": "leafo/lessphp",
              "version": "0.3.4",
              "source": {
                "type": "git",
                "url": "http://github.com/leafo/lessphp.git",
                "reference": "origin/master"
              }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "twitter/bootstrap",
              "version": "master",
              "source": {
                "type": "git",
                "url": "http://github.com/twitter/bootstrap.git",
                "reference": "origin/master"
              }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.*",
        "twitter/bootstrap": "dev-master",
        "leafo/lessphp" : "0.3.*"
    },
    "autoload": {
        "psr-0": { "PHPUGL\\TwitterBootstrapBundle": "" }
    },
    "target-dir": "PHPUGL/TwitterBootstrapBundle"
}
